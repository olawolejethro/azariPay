{
  "name": "AzariPay-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name --dataSource ./src/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run --dataSource ./src/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert --dataSource ./src/data-source.ts",
    "encrypt:data": "ts-node -r tsconfig-paths/register src/scripts/encrypt-existing-data.ts",
    "encrypt:direct": "ts-node -r tsconfig-paths/register src/scripts/encrypt-data-direct.ts",
    "encrypt:financial": "ts-node -r tsconfig-paths/register src/scripts/encrypt-financial-data.ts",
    "encrypt:p2p": "ts-node -r tsconfig-paths/register src/scripts/encrypt-p2p-seller-data.ts",
    "migrate:reference-hash": "ts-node -r tsconfig-paths/register src/scripts/add-reference-hashes.ts",
    "encrypt:p2p-chat": "ts-node -r tsconfig-paths/register src/scripts/encrypt-p2p-chat-content.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^2.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "country-state-city": "^3.2.1",
    "eventemitter2": "^6.4.9",
    "firebase-admin": "^13.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "sib-api-v3-sdk": "^8.5.0",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.4.0",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.15",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/twilio": "^3.19.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
